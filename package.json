{
  "name": "yalps",
  "version": "0.4.2",
  "description": "Yet another linear programming solver. (A rewrite of javascript-lp-solver.) Aims to be decently fast.",
  "homepage": "https://github.com/Ivordir/YALPS",
  "files": [
    "build"
  ],
  "main": "build/main/index.js",
  "module": "build/module/index.js",
  "exports": {
    "import": "./build/module/index.js",
    "require": "./build/main/index.js",
    "default": "./build/module/index.js"
  },
  "types": "build/types/index.d.ts",
  "sideEffects": false,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ivordir/YALPS.git"
  },
  "bugs": {
    "url": "https://github.com/Ivordir/YALPS/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Ivordir",
    "email": "ivordir@pm.me"
  },
  "keywords": [
    "Linear Programming",
    "Integer Programming",
    "Mixed Integer Linear Programming",
    "LP",
    "ILP",
    "MILP",
    "Solver",
    "Linear Optimization"
  ],
  "scripts": {
    "benchmark": "node --expose-gc --loader ts-node/esm benchmarks/benchmark.ts",
    "inspect": "node --inspect-brk --expose-gc --loader ts-node/esm",
    "test:main": "ts-mocha tests/tests.ts",
    "coverage": "c8 --check-coverage --lines 95 pnpm test:main",
    "test": "pnpm coverage",
    "start": "tsc -w",
    "build:esm": "tsc",
    "build:cjs": "tsc -m commonjs --outDir build/main",
    "build": "pnpm build:esm && pnpm build:cjs",
    "prepare": "pnpm build",
    "release": "np --no-cleanup"
  },
  "dependencies": {
    "heap": "^0.2.7"
  },
  "devDependencies": {
    "@types/heap": "^0.2.31",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.15.10",
    "c8": "^7.13.0",
    "glpk.js": "^4.0.1",
    "javascript-lp-solver": "^0.4.24",
    "mocha": "^10.2.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
